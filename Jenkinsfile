pipeline {
  agent any
  stages {
    stage('Dev') {
      steps {
        echo 'Code Dev pushed'
      }
    }

    stage('Test') {
      steps {
        echo 'Smoke Test completd'
      }
    }

    stage('Staging Envt') {
      steps {
        echo 'code stagged successfully'
      }
    }

  }
}